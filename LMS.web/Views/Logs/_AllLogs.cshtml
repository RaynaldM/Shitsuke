@using LMS.Resources
<section id="alllogs">
    @*<div class="table-responsive">*@
    <table id="grid-alllogs" class="table table-hover table-striped table-condensed">
        <thead>
            <tr>
                <th data-column-id="Id" data-type="numeric" data-formatter="link" data-identifier="true" data-order="desc" data-align="right"
                    data-cssclass="smallCell" data-headercssclass="smallCell">
                    @Global.LmsId
                </th>
                <th data-cssclass="mediumCell" data-headercssclass="mediumCell" data-column-id="Creationdate" data-type="datetime" data-formatter="datetime">
                    @Global.LmsCreationDate
                </th>
                <th data-column-id="Statuscode" data-cssclass="smallCell" data-headercssclass="smallCell">@Global.LogCode</th>
                <th data-column-id="Applicationid" data-cssclass="mediumCell" data-headercssclass="mediumCell">App</th>
                <th data-column-id="Type">@Global.LmsType</th>
                <th data-column-id="Message">@Global.LmsMessage</th>
                <th data-column-id="Url" data-cssclass="mediumCell" data-headercssclass="mediumCell">@Global.LmsUrl</th>
                <th data-column-id="Statuscode" data-cssclass="smallCell" data-headercssclass="smallCell">@Global.LogCode</th>
            </tr>
        </thead>
    </table>
    @*</div>*@
</section>
