@using LMS.Resources
<section id="occurence" class="hidden">
    @*<div class="table-responsive">*@
        <table id="grid-occurence" class="table table-hover table-striped table-condensed">
            <thead>
                <tr>
                    <th data-column-id="HashId" data-type="numeric" data-formatter="command" data-identifier="true" data-sortable="false"
                        data-cssclass="smallCell" data-headercssclass="smallCell"></th>
                    <th data-column-id="Count" data-type="numeric" data-order="desc" data-align="right"
                        data-cssclass="smallCell" data-headercssclass="smallCell">@Global.LmsCount</th>
                    <th  data-cssclass="mediumCell" data-headercssclass="mediumCell" data-column-id="LastDate" data-type="datetime" data-formatter="datetime">@Global.LogsLastDate</th>
                    <th data-column-id="Level" data-cssclass="smallCell" data-headercssclass="smallCell" data-formatter="level">@Global.LmsLevel</th>
                    <th data-column-id="Type">@Global.LmsType</th>
                    <th data-column-id="Message">@Global.LmsMessage</th>
                </tr>
            </thead>
        </table>
    @*</div>*@
</section>
